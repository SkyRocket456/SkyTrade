<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="650.0" prefWidth="500.0" style="-fx-background-color: E0E0E0;"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Login.AskShippingController">
    <children>
        <ImageView fitHeight="164.0" fitWidth="161.0" layoutX="170.0" layoutY="2.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@../../assets/SkyTrade-logos_black.png"/>
            </image>
        </ImageView>
        <Text layoutX="96.0" layoutY="240.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Shipping Address">
            <font>
                <Font size="20.0"/>
            </font>
        </Text>
        <TextField fx:id="shipping_address" layoutX="96.0" layoutY="246.0" prefHeight="25.0" prefWidth="308.0"/>
        <Text layoutX="96.0" layoutY="316.0" strokeType="OUTSIDE" strokeWidth="0.0" text="City">
            <font>
                <Font size="20.0"/>
            </font>
        </Text>
        <TextField fx:id="city" layoutX="96.0" layoutY="321.0" prefHeight="25.0" prefWidth="308.0"/>
        <Button layoutX="183.0" layoutY="540.0" mnemonicParsing="false" onAction="#ContinueShippingClick"
                prefHeight="52.0" prefWidth="135.0" style="-fx-background-color: rgb(245,117,106);" text="Continue">
            <font>
                <Font size="22.0"/>
            </font>
        </Button>
        <Text layoutX="53.0" layoutY="188.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter shipping information">
            <font>
                <Font name="Cambria" size="34.0"/>
            </font>
        </Text>
        <Text layoutX="96.0" layoutY="390.0" strokeType="OUTSIDE" strokeWidth="0.0" text="State">
            <font>
                <Font size="20.0"/>
            </font>
        </Text>
        <Text layoutX="96.0" layoutY="466.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Zip Code">
            <font>
                <Font size="20.0"/>
            </font>
        </Text>
        <TextField fx:id="zip_code" layoutX="96.0" layoutY="471.0" prefHeight="25.0" prefWidth="308.0"/>
        <ComboBox fx:id="states" layoutX="95.0" layoutY="395.0" prefHeight="25.0" prefWidth="308.0"/>
        <Text fx:id="shipping_addressError" fill="RED" layoutX="96.0" layoutY="288.0" strokeType="OUTSIDE"
              strokeWidth="0.0">
            <font>
                <Font size="16.0"/>
            </font>
        </Text>
        <Text fx:id="cityError" fill="RED" layoutX="96.0" layoutY="363.0" strokeType="OUTSIDE" strokeWidth="0.0">
            <font>
                <Font size="16.0"/>
            </font>
        </Text>
        <Text fx:id="statesError" fill="RED" layoutX="96.0" layoutY="437.0" strokeType="OUTSIDE" strokeWidth="0.0">
            <font>
                <Font size="16.0"/>
            </font>
        </Text>
        <Text fx:id="zipCodeError" fill="RED" layoutX="96.0" layoutY="513.0" strokeType="OUTSIDE" strokeWidth="0.0">
            <font>
                <Font size="16.0"/>
            </font>
        </Text>
    </children>
</AnchorPane>
